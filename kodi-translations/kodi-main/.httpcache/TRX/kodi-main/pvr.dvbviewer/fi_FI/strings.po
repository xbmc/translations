msgstr "tallennuskansion mukaan"

msgctxt "#30053"
msgid "by recording date"
msgstr "tallennuspäivän mukaan"

msgctxt "#30054"
msgid "by first letter"
msgstr "alkukirjaimen mukaan"

msgctxt "#30055"
msgid "by tv channel"
msgstr "tv-kanavan mukaan"

msgctxt "#30056"
msgid "by series"
msgstr "sarjan mukaan"

msgctxt "#30060"
msgid "Put outline (e.g. subtitles) before plot"
msgstr "Yhdistä juonitiivistelmä ja juoni"

msgctxt "#30061"
msgid "Never"
msgstr "Ei koskaan"

msgctxt "#30062"
msgid "In EPG only"
msgstr "Vain ohjelmaoppaassa"

msgctxt "#30063"
msgid "In recordings only"
msgstr "Vain tallennuksissa"

msgctxt "#30064"
msgid "Always"
msgstr "Aina"

msgctxt "#30100"
msgid "General"
msgstr "Yleiset"

msgctxt "#30101"
msgid "Advanced"
msgstr "Lisäasetukset"

msgctxt "#30102"
msgid "Recordings"
msgstr "Tallennukset"

msgctxt "#30500"
msgid "Unable to connect to the DVBViewer Recording Service"
msgstr "Yhteyden muodostaminen DVBViewer Recording Servicen kanssa epäonnistui"

msgctxt "#30501"
msgid "DVBViewer Recording Service version %s or higher required"
msgstr "DVBViewer Recording Servicestä vaaditaan versio %s tai uudempi"

msgctxt "#30502"
msgid "Unable to parse channel list"
msgstr "Kanavalistan lataaminen epäonnistui"

