<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="audiodecoder.nosefart"
  version="1.1.1"
  name="Nosefart Audio Decoder"
  provider-name="spiff">
  <requires>
    <import addon="kodi.audiodecoder" version="1.0.0"/>
  </requires>
  <extension
    point="kodi.audiodecoder"
    name="nsf"
    tags="true"
    tracks="true"
    extension=".nsf|.nsfstream"
    library_@PLATFORM@="@LIBRARY_FILENAME@"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en_GB">Nosefart (NSF) Audio Decoder</summary>
    <description lang="en_GB">Nosefart (NSF) Audio Decoder</description>
    <platform>@PLATFORM@</platform>
  </extension>
</addon>
